<h1>
  Discussion #<%= @discussion.id %> details
</h1>

<a href="/discussions/<%= @discussion.id %>/edit">
  Edit discussions
</a>

<hr>

<dl>
  <dt>
    Tab
  </dt>
  <dd>
    <%= @discussion.tab_id %>
  </dd>

  <dt>
    Guide
  </dt>
  <dd>
    <%= @discussion.guide_id %>
  </dd>

  <dt>
    Title
  </dt>
  <dd>
    <%= @discussion.title %>
  </dd>

  <dt>
    Created at
  </dt>
  <dd>
    <%= time_ago_in_words(@discussion.created_at) %> ago
  </dd>

  <dt>
    Updated at
  </dt>
  <dd>
    <%= time_ago_in_words(@discussion.updated_at) %> ago
  </dd>
</dl>

<a href="/delete_discussion/<%= @discussion.id %>">
  Delete discussion
</a>

<hr>

<a href="/discussions">
  Go back
</a>
